{
  "bar.layouts": {
    "*": {
      "left": [
        "dashboard",
        "clock",
        "media"
      ],
      "middle": [
        "workspaces"
      ],
      "right": [
        "volume",
        "network",
        "bluetooth",
        "battery",
        "notifications",
        "systray"
